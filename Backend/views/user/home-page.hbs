<script>
    // Function to apply zoomable text to all paragraphs in the "about" section
    function makeTextZoomable() {
        // Select all paragraphs within the about section
        const paragraphs = document.querySelectorAll('#about p');
        
        paragraphs.forEach(paragraph => {
            // Add the accessible-paragraph class
            paragraph.classList.add('accessible-paragraph');
            
            // Get the text content
            const text = paragraph.textContent;
            
            // Clear the paragraph
            paragraph.textContent = '';
            
            // Split text into words and wrap each in a span
            text.split(' ').forEach((word, index, array) => {
                const span = document.createElement('span');
                span.classList.add('zoomable-text');
                span.textContent = word + (index < array.length - 1 ? ' ' : '');
                paragraph.appendChild(span);
            });
        });
    }
    
    // Run the function when the DOM is fully loaded
    document.addEventListener('DOMContentLoaded', makeTextZoomable);
    
    // Alternative approach: make specific sections zoomable
    function makeSpecificTextZoomable(selector) {
        const elements = document.querySelectorAll(selector);
        elements.forEach(element => {
            // Get the text content
            const text = element.textContent;
            
            // Clear the element
            element.textContent = '';
            
            // Split text into words and wrap each in a span
            text.split(' ').forEach((word, index, array) => {
                const span = document.createElement('span');
                span.classList.add('zoomable-text');
                span.textContent = word + (index < array.length - 1 ? ' ' : '');
                element.appendChild(span);
            });
        });
    }
    
    // Uncomment the line below if you want to apply zoom to specific elements instead of all paragraphs
    // document.addEventListener('DOMContentLoaded', () => makeSpecificTextZoomable('#about .section-header p, #about .about-col p'));
</script>

